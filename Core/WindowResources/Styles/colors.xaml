<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#51aec8-->
    <Color x:Key="BackgroundLightBlue">#42afcf</Color>
    <SolidColorBrush x:Key="AppBackgroundMainBrush" Color="{StaticResource BackgroundLightBlue}"/>

    <Color x:Key="BackgroundDarkBlue">#347ca3</Color>
    <SolidColorBrush x:Key="AppBackgroundSecondaryBrush" Color="{StaticResource BackgroundDarkBlue}"/>

    <Color x:Key="BackgroundLightGreen">#54c49f</Color>
    <SolidColorBrush x:Key="AppElementBackgroundBrush" Color="{StaticResource BackgroundLightGreen}"/>

    <Color x:Key="ForegroundWhite">#ffff</Color>
    <SolidColorBrush x:Key="AppForegroundBrush" Color="{StaticResource ForegroundWhite}"/>

    <SolidColorBrush x:Key="Item.MouseOver.Background" Color="{StaticResource BackgroundLightGreen}"/>
    <SolidColorBrush x:Key="Item.MouseOver.Border" Color="#a826A0Da"/>
    <SolidColorBrush x:Key="Item.SelectedInactive.Background" Color="Red"/>
    <SolidColorBrush x:Key="Item.SelectedInactive.Border" Color="Red"/>
    <SolidColorBrush x:Key="Item.SelectedActive.Background" Color="{StaticResource BackgroundLightBlue}"/>
    <SolidColorBrush x:Key="Item.SelectedActive.Border" Color="#FF26A0DA"/>
</ResourceDictionary>