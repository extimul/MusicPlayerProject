<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundLightBlue">#0B5FA5</Color>
    <SolidColorBrush x:Key="AppBackgroundMainBrush" Color="{StaticResource BackgroundLightBlue}"/>

    <Color x:Key="BackgroundDarkBlue">#66A1D2</Color>
    <SolidColorBrush x:Key="AppBackgroundSecondaryBrush" Color="{StaticResource BackgroundDarkBlue}"/>

    <Color x:Key="BackgroundLightGreen">#35D4A0</Color>
    <SolidColorBrush x:Key="AppElementBackgroundBrush" Color="{StaticResource BackgroundLightGreen}"/>

    <Color x:Key="ForegroundWhite">#ffff</Color>
    <SolidColorBrush x:Key="AppForegroundBrush" Color="{StaticResource ForegroundWhite}"/>
</ResourceDictionary>